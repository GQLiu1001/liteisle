# 生产环境配置
VITE_APP_TITLE=Liteisle Desktop
VITE_APP_VERSION=1.0.0

# API配置 - 请修改为你的后端服务器地址
VITE_API_BASE_URL=http://localhost:8002
VITE_WS_BASE_URL=ws://localhost:8002

# 应用配置
VITE_APP_ENV=production
VITE_DEBUG=false

# 上传配置
VITE_UPLOAD_MAX_SIZE=100
VITE_UPLOAD_ALLOWED_TYPES=jpg,jpeg,png,gif,pdf,doc,docx,txt,md

# 功能开关
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_PICGO=true
VITE_ENABLE_FOCUS_MODE=true

# 生产环境特有配置
VITE_ENABLE_DEV_TOOLS=false
VITE_MOCK_DATA=false
