<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liteisle.mapper.UsersMapper">

    <resultMap id="BaseResultMap" type="com.liteisle.common.domain.Users">
            <id property="id" column="id" />
            <result property="username" column="username" />
            <result property="email" column="email" />
            <result property="password" column="password" />
            <result property="avatar" column="avatar" />
            <result property="storageQuota" column="storage_quota" />
            <result property="storageUsed" column="storage_used" />
            <result property="createTime" column="create_time" />
            <result property="updateTime" column="update_time" />
    </resultMap>

    <sql id="Base_Column_List">
        id,username,email,password,avatar,storage_quota,
        storage_used,create_time,update_time
    </sql>
</mapper>
